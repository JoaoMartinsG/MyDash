<h1>All vehicles</h1>

<% @vehicles.each do |vehicle| %>
  <h2><strong>Owner:</strong> <%= vehicle.user.first_name %> <%= vehicle.user.last_name %></h2>
  <h3><strong>Make: </strong><%= vehicle.make %></h3>
  <h3><strong>Model: </strong><%= vehicle.model %></h3>
  <h3><strong>Plate: </strong><%= vehicle.plate %></h3>
  <h3><strong>year: </strong><%= vehicle.year %></h3>
  <h3><strong>Mileage: </strong><%= vehicle.mileage %> Km</h3>
  <h3><strong>Driver: </strong><%= vehicle.driver %></h3>



<% end %>
