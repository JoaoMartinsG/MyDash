<h1>All vehicles</h1>
<div class="container">
<div class="row">



<% @vehicles.each do |vehicle| %>
  <div class="col-xs-12 col-md-4 col-offset-4">
   <div class="card-product">
      <img src="https://raw.githubusercontent.com/lewagon/fullstack-images/master/uikit/skateboard.jpg" />
      <div class="card-product-infos">
        <h2><%= vehicle.make %></h2>
        <p>Driver: <%= vehicle.driver %></p>
        <p>Plate: <%= vehicle.plate %></p>
        <p>mkm</p>
        <p><%= "dsdsdsdjhsndkjsndkjnskjdnskjndkjsndkjndkj"[0, 20] + "..."; %></p>
        <p>konokn</p>
      </div>
      <div class="card-product-alart">
        <p>3</p>
        <p>alerts</p>
      </div>
    </div>
    <br>
 </div>
<% end %>
</div>
</div>
